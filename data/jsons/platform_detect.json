{
  "platforms": [
    {
     "name": "intel-galileo-rev-d",
     "validation": [
      {
       "file_path": "/sys/devices/virtual/dmi/id/board_name",
       "match": [ "Galileo$" ]
      }
     ]
    },
    {
     "name": "intel-galileo-rev-g",
     "validation": [
      {
       "file_path": "/sys/devices/virtual/dmi/id/board_name",
       "match": [ "GalileoGen2" ]
      }
     ]
    },
    {
     "name": "intel-edison-rev-c",
     "validation": [
      {
       "file_path": "/sys/devices/virtual/dmi/id/board_name",
       "match": [ "BODEGA BAY" ]
      }
     ]
    },
    {
     "name": "intel-edison-rev-c",
     "validation": [
      {
       "file_path": "/sys/devices/virtual/dmi/id/board_name",
       "match": [ "SALT BAY" ]
      }
     ]
    }
  ]
}

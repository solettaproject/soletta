	flow/netctl.mod

#connection

sample-network-connect-$(FLOW_NETCTL_SAMPLE) := \
	network-connect.fbp

sample-network-disconnect-$(FLOW_NETCTL_SAMPLE) := \
	network-disconnect.fbp


sample-$(FLOW_NETCTL_SAMPLE) += \
	list-network-services \
	network-connect \
	network-disconnect \
	network-radios

# list

sample-list-network-services-$(FLOW_NETCTL_SAMPLE) := \
	list-network-services.fbp


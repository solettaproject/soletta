ten(constant/int:value=10)
five(constant/int:value=5)
seventeen(constant/int:value=17)

five OUT -> IN0 equal(int/equal)
five OUT -> IN1 equal
equal OUT -> RESULT r_equal(test/result)

ten OUT -> IN0 not_equal(int/not-equal)
seventeen OUT -> IN1 not_equal
not_equal OUT -> RESULT r_not_equal(test/result)

ten OUT -> IN0 greater(int/greater)
five OUT -> IN1 greater
greater OUT -> RESULT r_greater(test/result)

ten OUT -> IN0 greater_or_equal_greater(int/greater-or-equal)
five OUT -> IN1 greater_or_equal_greater
greater_or_equal_greater OUT -> RESULT r_greater_or_equal_greater(test/result)

five OUT -> IN0 greater_or_equal_equal(int/greater-or-equal)
five OUT -> IN1 greater_or_equal_equal
greater_or_equal_equal OUT -> RESULT r_greater_or_equal_equal(test/result)

five OUT -> IN0 less(int/less)
seventeen OUT -> IN1 less
less OUT -> RESULT r_less(test/result)

five OUT -> IN0 less_or_equal_less(int/less-or-equal)
ten OUT -> IN1 less_or_equal_less
less_or_equal_less OUT -> RESULT r_less_or_equal_less(test/result)

seventeen OUT -> IN0 less_or_equal_equal(int/less-or-equal)
seventeen OUT -> IN1 less_or_equal_equal
less_or_equal_equal OUT -> RESULT r_less_or_equal_equal(test/result)


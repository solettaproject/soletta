XByte(constant/byte:value=20)
YByte(constant/byte:value=40)
ZByte(constant/byte:value=70)
byte_to_vector_3f(converter/byte-to-vector-3f)

XByte OUT -> X byte_to_vector_3f
YByte OUT -> Y byte_to_vector_3f
ZByte OUT -> Z byte_to_vector_3f

match_twenty(constant/int:value=20)
match_eighty(constant/int:value=80)
match_seventy(constant/int:value=70)

byte_to_vector_3f OUT -> IN vector_3f_to_byte(converter/vector-3f-to-byte)

vector_3f_to_byte X -> IN _(converter/byte-to-int) OUT -> IN0 eq_twenty(int/equal)
match_twenty OUT -> IN1 eq_twenty OUT -> RESULT x(test/result)

vector_3f_to_byte Y -> IN _(converter/byte-to-int) OUT -> IN0 eq_eighty(int/equal)
match_eighty OUT -> IN1 eq_eighty OUT -> IN _(boolean/not) OUT -> RESULT y(test/result)

vector_3f_to_byte Z -> IN _(converter/byte-to-int) OUT -> IN0 eq_seventy(int/equal)
match_seventy OUT -> IN1 eq_seventy OUT -> RESULT z(test/result)

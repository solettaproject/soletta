      },
      "options": {
        "members": [
          {
            "data_type": "string",
            "default": null,
            "description": "The url to make a GET request",
            "name": "url"
          },
          {
            "data_type": "boolean",
            "description": "If the machine_id should be set on every request made",
            "name": "machine_id",
            "default": false
          },
          {
            "data_type": "boolean",
            "default": false,
            "description": "Enforce the response content type, if it does not match if the requested one, ignore it.",
            "name": "strict"
          },
          {
            "data_type": "string",
            "default": "text/plain",
            "description": "The desired content type of the request",
            "name": "content_type"
          }
        ],
        "version": 1

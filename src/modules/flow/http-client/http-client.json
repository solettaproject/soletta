          "methods": {
            "process": "common_get_process"
          },
          "name": "GET"
        },
        {
          "data_type": "boolean",
          "description": "Post into the url",
          "methods": {
            "process": "boolean_post_process"
          },
          "name": "POST"
        }
      ],

{
  "$schema": "http://solettaproject.github.io/soletta/schemas/node-type-genspec.schema",
  "name": "constant",
  "meta": {
    "author": "Intel Corporation",
    "license": "BSD-3-Clause",
    "version": "1"
  },
  "types": [
    {
      "category": "constant",
      "description": "Send an initial packet with a constant value set on options.",
      "methods": {
        "open": "constant_boolean_open"
      },
      "name": "constant/boolean",
      "options": {
        "members": [
          {
            "data_type": "boolean",
            "description": "Value of constant.",
            "name": "value"
          }
        ],
        "version": 1
      },
      "out_ports": [
        {
          "data_type": "boolean",
          "description": "Constant value, sent only once, when connected to a node.",
          "name": "OUT"
        }
      ],
      "url": "http://solettaproject.org/doc/latest/node_types/constant/boolean.html"
    },
    {
      "category": "constant",
      "description": "Send an initial packet with a constant value set on options.",
      "methods": {
        "open": "constant_byte_open"
      },
      "name": "constant/byte",
      "options": {
        "members": [
          {
            "data_type": "byte",
            "description": "Value of constant.",
            "name": "value"
          }
        ],
        "version": 1
      },
      "out_ports": [
        {
          "data_type": "byte",
          "description": "Constant value, sent only once, when connected to a node.",
          "name": "OUT"
        }
      ],
      "url": "http://solettaproject.org/doc/latest/node_types/constant/byte.html"
    },
    {
      "category": "constant",
      "description": "Send an initial packet with a constant value set on options.",
      "methods": {
        "open": "constant_drange_open"
      },
      "name": "constant/float",
      "options": {
        "members": [
          {
            "data_type": "float",
            "default": 0,
            "description": "Value of constant.",
            "name": "value"
          },
          {
            "data_type": "drange-spec",
            "default": {
                "min": "-DBL_MAX",
                "max": "DBL_MAX",
                "step": "DBL_MIN"
            },
            "description": "Range (min / max) and step of constant.",
            "name": "value_spec"
          }
        ],
        "version": 1
      },
      "out_ports": [
        {
          "data_type": "float",
          "description": "Constant value, sent only once, when connected to a node.",
          "name": "OUT"
        }
      ],
      "url": "http://solettaproject.org/doc/latest/node_types/constant/drange.html"
    },
    {
      "category": "constant",
      "description": "Send an initial empty packet.",
      "methods": {
        "open": "constant_empty_open"
      },
      "name": "constant/empty",
      "out_ports": [
        {
          "data_type": "empty",
          "description": "A single pulse sent when connected to a node.",
          "name": "OUT"
        }
      ],
      "url": "http://solettaproject.org/doc/latest/node_types/constant/empty.html"
    },
    {
      "category": "constant",
      "description": "Send an initial packet with a constant value set on options.",
      "methods": {
        "open": "constant_irange_open"
      },
      "name": "constant/int",
      "options": {
        "members": [
          {
            "data_type": "int",
            "default": 0,
            "description": "Value of constant.",
            "name": "value"
          },
          {
            "data_type": "irange-spec",
            "default": {
                "min": "INT32_MIN",
                "max": "INT32_MAX",
                "step": 1
            },
            "description": "Range (min / max) and step of constant.",
            "name": "value_spec"
          }
        ],
        "version": 1
      },
      "out_ports": [
        {
          "data_type": "int",
          "description": "Constant value, sent only once, when connected to a node.",
          "name": "OUT"
        }
      ],
      "url": "http://solettaproject.org/doc/latest/node_types/constant/irange.html"
    },
    {
      "category": "constant",
      "description": "Send an initial packet with a constant value set on options.",
      "methods": {
        "open": "constant_rgb_open"
      },
      "name": "constant/rgb",
      "options": {
        "members": [
          {
            "data_type": "rgb",
            "description": "Value of constant.",
            "name": "value"
          }
        ],
        "version": 1
      },
      "out_ports": [
        {
          "data_type": "rgb",
          "description": "Constant value, sent only once, when connected to a node.",
          "name": "OUT"
        }
      ],
      "url": "http://solettaproject.org/doc/latest/node_types/constant/rgb.html"
    },
    {
      "category": "constant",
      "description": "Send an initial packet with a constant value set on options.",
      "methods": {
        "open": "constant_direction_vector_open"
      },
      "name": "constant/direction-vector",
      "options": {
        "members": [
          {
            "data_type": "direction-vector",
            "description": "Value of constant.",
            "name": "value"
          }
        ],
        "version": 1
      },
      "out_ports": [
        {
          "data_type": "direction-vector",
          "description": "Constant value, sent only once, when connected to a node.",
          "name": "OUT"
        }
      ],
      "private_data_type": "sol_direction_vector",
      "url": "http://solettaproject.org/doc/latest/node_types/constant/direction-vector.html"
    },
    {
      "category": "constant",
      "description": "Send an initial packet with a string set on options.",
      "methods": {
        "open": "constant_string_open"
      },
      "name": "constant/string",
      "options": {
        "members": [
          {
            "data_type": "string",
            "description": "String to be sent when node is connected.",
            "name": "value"
          }
        ],
        "version": 1
      },
      "out_ports": [
        {
          "data_type": "string",
          "description": "Constant value, sent only once, when connected to a node.",
          "name": "OUT"
        }
      ],
      "url": "http://solettaproject.org/doc/latest/node_types/constant/string.html"
    }
  ]
}
